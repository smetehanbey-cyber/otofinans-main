{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install",
  "functions": {
    "api/[...].ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "framework": null,
  "devCommand": "pnpm dev",
  "trailingSlash": false
}
